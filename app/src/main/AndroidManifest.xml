<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.chat.registrocloud">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="SistChat"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name="app.chat.registrocloud.Activity.Ver_grupos"></activity>
        <activity android:name="app.chat.registrocloud.Activity.chatgrupal" />
        <activity android:name="app.chat.registrocloud.Activity.MensajeriaActivity" />
        <activity android:name="app.chat.registrocloud.Activity.RegistroActivity" />
        <activity android:name="app.chat.registrocloud.Activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>